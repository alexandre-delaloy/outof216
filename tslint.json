{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "radix": false,
        "no-string-literal": false,
        "prefer-for-of": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false
    },
    "rulesDirectory": []
}